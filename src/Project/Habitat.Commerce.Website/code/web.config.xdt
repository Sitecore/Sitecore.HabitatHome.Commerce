          <dependentAssembly xdt:Transform="Insert">
            <assemblyIdentity name="Microsoft.OData.Client" publicKeyToken="31bf3856ad364e35" xmlns="urn:schemas-microsoft-com:asm.v1" />
            <bindingRedirect oldVersion="6.0.0.0-7.4.4.20330" newVersion="7.4.4.20330" xmlns="urn:schemas-microsoft-com:asm.v1" />
          </dependentAssembly>
